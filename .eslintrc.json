{
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "parserOptions": {
    "ecmaVersion": 5,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "semi": [2, "never"],
    "no-trailing-spaces": "error",
    "quotes": ["error", "double"],
    "indent": ["error", 2, {"SwitchCase": 1}],
    "curly": "error",
    "react/prop-types": [2, {"skipUndeclared": true}]
  }
}
