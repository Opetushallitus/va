{:environment              "aws-dev"
 :server                  {:hakija-host     "*"
                           :virkailija-host "*"
                           :url             {:fi "https://dev.valtionavustukset.oph.fi/"
                                             :sv "https://dev.statsunderstod.oph.fi/"}
                           :virkailija-url  "https://dev.virkailija.valtionavustukset.oph.fi"}
 :email                   {:enabled?         false}
 :rondo-scheduler         {:enabled?                 false
                           :scheduling               "0 00 04 ? * *"
