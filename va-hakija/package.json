{
  "name": "va-web",
  "version": "1.0.0",
  "description": "OPH Valtionavustus Web UI",
  "license": "EUPL-1.1",
  "repository": {
    "type": "git",
    "url": "https://github.com:Opetushallitus/va.git"
  },
  "dependencies": {
    "va-common": "file:../va-common",
    "baconjs": "^0.7.59",
    "bowser": "^0.7.3",
    "classnames": "^2.1.3",
    "console-polyfill": "^0.2.1",
    "factor-bundle": "^2.5.0",
    "lodash": "^3.9.3",
    "query-string": "2.2.0",
    "react": "^0.13.2",
    "timeout-transition-group": "1.0.1",
    "seamless-immutable": "^2.4.2",
    "traverse": "0.6.6",
    "moment-timezone": "0.4.0",
    "route-parser": "^0.0.4",
    "react-dropzone-es6": "1.0.1"
  },
  "devDependencies": {
    "babel": "^5.6.14",
    "babel-core": "^5.8.20",
    "babel-loader": "^5.3.2",
    "chai": "^3.0.0",
    "css-loader": "^0.15.6",
    "file-loader": "^0.8.4",
    "jsdom": "3.1.2",
    "less": "^2.5.1",
    "less-loader": "^2.2.0",
    "mocha": "^2.2.5",
    "mocha-jsdom": "^0.5.0",
    "mocha-junit-reporter": "^1.5.0",
    "mocha-phantomjs": "3.5.6",
    "phantomjs": "1.9.17",
    "react-tools": "^0.13.2",
    "style-loader": "^0.12.3",
    "url-loader": "^0.5.6",
    "json-loader": "^0.5.2",
    "watch-spawn": "^1.0.5",
    "webpack": "^1.10.5"
  },
  "private": false,
  "scripts": {
    "unittest": "mocha --compilers js:babel/register web/test/*Test.js",
    "watch-unittest": "mocha --watch --compilers js:babel/register web/test/*Test.js",
    "build": "UGLIFY=true webpack -p",
    "dev-build": "webpack -d --progress --colors",
    "watch-web": "watch-spawn -d 300 -i -p 'node_modules/va-common/package.json' -p 'web/**/*' -p 'package.json' npm run dev-build",
    "update-common-web": "rm -rf node_modules/va-common && npm install va-common",
    "watch-common-web": "watch-spawn -p '../va-common/web/**/*' -p '../va-common/package.json' npm run update-common-web",
    "watch": "npm run update-common-web && (npm run watch-common-web & npm run watch-web)"
  }
}
