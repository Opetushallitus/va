  "scripts": {
    "common:lint": "cd va-common && eslint --ignore-path ../.gitignore --ignore-pattern '*.json' --ignore-pattern '**/test/lib' '**/*.js*'",
    "common:test": "cd va-common && mocha --require web/test/babelhook $MOCHA_ARGS web/test/*Test.js",
    "common:test-watch": "cd va-common && mocha --watch --require web/test/babelhook $MOCHA_ARGS web/test/*Test.js",
    "common:snyk-protect": "cd va-common && snyk protect",
