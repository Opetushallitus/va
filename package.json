    "hakija:snyk-protect": "cd va-hakija && snyk protect",
    "hakija:prepare": "npm run hakija:snyk-protect",
    "virkailija:build": "cd va-virkailija && webpack --bail",
    "virkailija:build-production": "NODE_ENV=production npm run virkailija:build",
    "virkailija:build-watch": "cd va-virkailija && webpack --progress --watch",
    "virkailija:lint": "cd va-virkailija && eslint --ignore-path ../.gitignore --ignore-pattern '*.json' --ignore-pattern '**/test/lib' '**/*.jsx?'",
